			CACCCGGGGCCCCCATCTCCGGGCGGAAGGTACCATCGATGAGGCGGCCTGAGGGCGACAGGCCACTTAACGCCGACT
		</sequence>
		<sequence>
			<taxon idref="env383_2013"/>
			CACCCGGGGCCCCCATCTCCGGGCGGAAGGTGCCATCGATGAGGCGGCCTGAGGGCGACAGGCCACTTAACGCCGACT
		</sequence>
		<sequence>
			<taxon idref="env4303_2015"/>
			TATCCGGGGCCCCCATCTCCGGACGGAAGGT?CCATCGGGAAGACGGCCTGCGGGCGATAGGCCCCTTAATTCAGACT
		</sequence>
		<sequence>
			<taxon idref="env4449_2015"/>
			TATCCGGGGCCCCAAGCTCCGGACGGAAGGTACCATCGGGGAGACGGCCTGCGGGCGATAGGCTACTTAACTCAGATT
		</sequence>
		<sequence>
			<taxon idref="env4454_2015"/>
			TATCCGGGGCCCCAAGCTCCGGACGGAAGGT?CCATCGGGGAGACGGCCTGCGGGCGATAGGCTACTTAACTCAGATT
		</sequence>
		<sequence>
			<taxon idref="env894_2013"/>
			CACCCGGAGCCCCCATCTCCGGGCGGGAGGTATCATCGATGATGTGGCCCGAGGGCGGCAGGCCACTTAACGCCGACT
		</sequence>
		<sequence>
			<taxon idref="env898_2013"/>
			CACCCGGAGCCCCCATCTCCGGGCGGGAGGTATCATCGATGATGTGGCCCGAGGGCGGCAGGCCACTTAACGCCGACT
		</sequence>
		<sequence>
			<taxon idref="env901_2013"/>
			CACCCGGAGCCCCCATCTCCGGGCGGGAGGTATCATCGATGATGTGGCCCGAGGGCGGCAGGCCACTTAACGCCGACT
		</sequence>
		<sequence>
			<taxon idref="env90_2012"/>
			CACCCGGGGCCCCCATCTCCGGGCGGGAGGTATCATCGATGATGTGGCCTGAGGGCGACAGGCCACTTAACGCCGACT
		</sequence>
		<sequence>
			<taxon idref="env949_2013"/>
			CACTCGGAGCCCCCATCTCCGGGCGGGAGGTATCATCGATGATGTGGCCCGAGGGCGACAGGCCACTTAACGCCGACT
		</sequence>
		<sequence>
			<taxon idref="env94_2012"/>
			CACCCGGGGCCCCCATCTCCGGGCGGGAGGTATCATCGATGATGTGGCCTGAGGGCGACAGGCCACTTAACGCCGACT
		</sequence>
		<sequence>
			<taxon idref="env955_2013"/>
			CACCCGGAGCTCCCATCTCCGGGCGGGAGGTATCATCAATGATGTGGCCCGAGGGCGACAGGCCACTTAGCGCCGACT
		</sequence>
		<sequence>
			<taxon idref="env961_2013"/>
			TATCCGGGGCCCCCATCTCCGGACGGAAGGTACCATCGGTGAGACGGCCTGCGGGCGACAGGCCACTTAACGCAGACT
		</sequence>
	</alignment>

<ascertainedPatterns id="patterns" from="5">
    <alignment idref="alignment"/>
    <!-- all constant sites (1 through 4) are excluded from the dataset     -->
    <excludePatterns from="1" to="4"/>
</ascertainedPatterns>

	<!-- START Discrete Traits Model                                             -->

	<!-- general data type for discrete trait model, 'Location'                  -->
	<generalDataType id="Location.dataType">

		<!-- Number Of States = 2                                                    -->
		<state code="C"/>
		<state code="E"/>
	</generalDataType>


	<!-- Data pattern for discrete trait, 'Location'                             -->
	<attributePatterns id="Location.pattern" attribute="Location">
		<taxa idref="taxa"/>
		<generalDataType idref="Location.dataType"/>
	</attributePatterns>

	<!-- END Discrete Traits Model                                               -->


	<!-- This is a simple constant population size coalescent model              -->
	<!-- that is used to generate an initial tree for the chain.                 -->
	<constantSize id="initialDemo" units="years">
		<populationSize>
			<parameter id="initialDemo.popSize" value="100.0"/>
		</populationSize>
	</constantSize>

	<!-- Generate a random starting tree under the coalescent process            -->
	<coalescentSimulator id="startingTree">
		<taxa idref="taxa"/>
		<constantSize idref="initialDemo"/>
	</coalescentSimulator>

	<!-- Generate a tree model                                                   -->
	<treeModel id="treeModel">
		<coalescentTree idref="startingTree"/>
		<rootHeight>
			<parameter id="treeModel.rootHeight"/>
		</rootHeight>
		<nodeHeights internalNodes="true">
			<parameter id="treeModel.internalNodeHeights"/>
		</nodeHeights>
		<nodeHeights internalNodes="true" rootNode="true">
			<parameter id="treeModel.allInternalNodeHeights"/>
		</nodeHeights>
	</treeModel>

	<!-- Generate a gmrfSkyrideLikelihood for GMRF Bayesian Skyride process      -->
	<gmrfSkyrideLikelihood id="skyride" timeAwareSmoothing="true" randomizeTree="false">
		<populationSizes>

			<!-- skyride.logPopSize is in log units unlike other popSize                 -->
			<parameter id="skyride.logPopSize" dimension="64" value="3.9318256327243257"/>
		</populationSizes>
		<groupSizes>
			<parameter id="skyride.groupSize" dimension="64"/>
		</groupSizes>
		<precisionParameter>
			<parameter id="skyride.precision" value="1.0" lower="0.0"/>
		</precisionParameter>
		<populationTree>
			<treeModel idref="treeModel"/>
		</populationTree>
	</gmrfSkyrideLikelihood>

	<!-- The strict clock (Uniform rates across branches)                        -->
	<strictClockBranchRates id="vc_rep1_new_CM.branchRates">
		<rate>
			<parameter id="vc_rep1_new_CM.clock.rate" value="1.0" lower="0.0"/>
		</rate>
	</strictClockBranchRates>
	<rateStatistic id="vc_rep1_new_CM.meanRate" name="vc_rep1_new_CM.meanRate" mode="mean" internal="true" external="true">
		<treeModel idref="treeModel"/>
		<strictClockBranchRates idref="vc_rep1_new_CM.branchRates"/>
	</rateStatistic>

	<!-- The strict clock (Uniform rates across branches)                        -->
	<strictClockBranchRates id="Location.branchRates">
		<rate>
			<parameter id="Location.clock.rate" value="1.0" lower="0.0"/>
		</rate>
	</strictClockBranchRates>
	<rateStatistic id="Location.meanRate" name="Location.meanRate" mode="mean" internal="true" external="true">
		<treeModel idref="treeModel"/>
		<strictClockBranchRates idref="Location.branchRates"/>
	</rateStatistic>

	<!-- The HKY substitution model (Hasegawa, Kishino & Yano, 1985)             -->
	<HKYModel id="hky">
		<frequencies>
			<frequencyModel dataType="nucleotide">
				<alignment idref="alignment"/>
				<frequencies>
					<parameter id="frequencies" dimension="4"/>
				</frequencies>
			</frequencyModel>
		</frequencies>
		<kappa>
			<parameter id="kappa" value="2.0" lower="0.0"/>
		</kappa>
	</HKYModel>

	<!-- site model                                                              -->
	<siteModel id="siteModel">
		<substitutionModel>
			<HKYModel idref="hky"/>
		</substitutionModel>
		<gammaShape gammaCategories="4">
			<parameter id="alpha" value="0.5" lower="0.0"/>
		</gammaShape>
	</siteModel>

	<!-- Likelihood for tree given sequence data                                 -->
	<treeLikelihood id="treeLikelihood" useAmbiguities="false">
		<patterns idref="patterns"/>
		<treeModel idref="treeModel"/>
		<siteModel idref="siteModel"/>
		<strictClockBranchRates idref="vc_rep1_new_CM.branchRates"/>
	</treeLikelihood>

	<!-- START Discrete Traits Model                                             -->

	<!-- symmetric CTMC model for discrete state reconstructions                 -->
	<generalSubstitutionModel id="Location.model">
		<generalDataType idref="Location.dataType"/>
		<frequencies>
			<frequencyModel id="Location.frequencyModel" normalize="true">
				<generalDataType idref="Location.dataType"/>
				<frequencies>
					<parameter id="Location.frequencies" dimension="2"/>
				</frequencies>
			</frequencyModel>
		</frequencies>

		<!-- rates and indicators                                                    -->
		<rates>
			<parameter id="Location.rates" value="1.0"/>
		</rates>
	</generalSubstitutionModel>
	<siteModel id="Location.siteModel">
		<substitutionModel>
			<generalSubstitutionModel idref="Location.model"/>
		</substitutionModel>
	</siteModel>

	<!-- Likelihood for tree given discrete trait data                           -->
	<ancestralTreeLikelihood id="Location.treeLikelihood" stateTagName="Location.states">
		<attributePatterns idref="Location.pattern"/>
		<treeModel idref="treeModel"/>
		<siteModel idref="Location.siteModel"/>
		<generalSubstitutionModel idref="Location.model"/>
		<strictClockBranchRates idref="Location.branchRates"/>
	</ancestralTreeLikelihood>

	<!-- END Discrete Traits Model                                               -->


	<!-- Define operators                                                        -->
	<operators id="operators" optimizationSchedule="log">
		<scaleOperator scaleFactor="0.75" weight="1">
			<parameter idref="kappa"/>
		</scaleOperator>
		<scaleOperator scaleFactor="0.75" weight="1">
			<parameter idref="alpha"/>
		</scaleOperator>
		<scaleOperator scaleFactor="0.75" weight="30">
			<parameter idref="vc_rep1_new_CM.clock.rate"/>
		</scaleOperator>
		<scaleOperator scaleFactor="0.75" weight="30">
			<parameter idref="Location.clock.rate"/>
		</scaleOperator>
		<subtreeSlide size="5.1" gaussian="true" weight="150">
			<treeModel idref="treeModel"/>
		</subtreeSlide>
		<narrowExchange weight="150">
			<treeModel idref="treeModel"/>
		</narrowExchange>
		<wideExchange weight="30">
			<treeModel idref="treeModel"/>
		</wideExchange>
		<wilsonBalding weight="30">
			<treeModel idref="treeModel"/>
		</wilsonBalding>
		<scaleOperator scaleFactor="0.75" weight="30">
			<parameter idref="treeModel.rootHeight"/>
		</scaleOperator>
		<uniformOperator weight="300">
			<parameter idref="treeModel.internalNodeHeights"/>
		</uniformOperator>
		<gmrfBlockUpdateOperator scaleFactor="2.0" weight="2">
			<gmrfSkyrideLikelihood idref="skyride"/>
		</gmrfBlockUpdateOperator>
		<upDownOperator scaleFactor="0.75" weight="30">
			<up>
				<parameter idref="vc_rep1_new_CM.clock.rate"/>
			</up>
			<down>
				<parameter idref="treeModel.allInternalNodeHeights"/>
			</down>
		</upDownOperator>
		<upDownOperator scaleFactor="0.75" weight="30">
			<up>
				<parameter idref="Location.clock.rate"/>
			</up>
			<down>
				<parameter idref="treeModel.allInternalNodeHeights"/>
			</down>
		</upDownOperator>
		<scaleOperator scaleFactor="0.75" weight="15" scaleAllIndependently="true">
			<parameter idref="Location.rates"/>
		</scaleOperator>
	</operators>

	<!-- Define MCMC                                                             -->
	<mcmc id="mcmc" chainLength="50000000" autoOptimize="true" operatorAnalysis="vc_rep1_new_CM_SC_BSR_phylo_simmNoBSSVS_50.ops">
		<posterior id="posterior">
			<prior id="prior">
				<logNormalPrior mean="1.0" stdev="1.25" offset="0.0" meanInRealSpace="false">
					<parameter idref="kappa"/>
				</logNormalPrior>
				<exponentialPrior mean="0.5" offset="0.0">
					<parameter idref="alpha"/>
				</exponentialPrior>
				<ctmcScalePrior>
					<ctmcScale>
						<parameter idref="vc_rep1_new_CM.clock.rate"/>
					</ctmcScale>
					<treeModel idref="treeModel"/>
				</ctmcScalePrior>
				<ctmcScalePrior>
					<ctmcScale>
						<parameter idref="Location.clock.rate"/>
					</ctmcScale>
					<treeModel idref="treeModel"/>
				</ctmcScalePrior>
				<gammaPrior shape="0.001" scale="1000.0" offset="0.0">
					<parameter idref="skyride.precision"/>
				</gammaPrior>
				<uniformPrior lower="0.0" upper="1.0">
					<parameter idref="Location.frequencies"/>
				</uniformPrior>
				<cachedPrior>
					<gammaPrior shape="1.0" scale="1.0" offset="0.0">
						<parameter idref="Location.rates"/>
					</gammaPrior>
					<parameter idref="Location.rates"/>
				</cachedPrior>
				<gmrfSkyrideLikelihood idref="skyride"/>
			</prior>
			<likelihood id="likelihood">
				<treeLikelihood idref="treeLikelihood"/>
				<strictClockBranchRates idref="vc_rep1_new_CM.branchRates"/>
				<strictClockBranchRates idref="Location.branchRates"/>

				<!-- START Discrete Traits Model                                             -->
				<ancestralTreeLikelihood idref="Location.treeLikelihood"/>

				<!-- END Discrete Traits Model                                               -->

			</likelihood>
		</posterior>
		<operators idref="operators"/>

		<!-- write log to screen                                                     -->
		<log id="screenLog" logEvery="5000">
			<column label="Posterior" dp="4" width="12">
				<posterior idref="posterior"/>
			</column>
			<column label="Prior" dp="4" width="12">
				<prior idref="prior"/>
			</column>
			<column label="Likelihood" dp="4" width="12">
				<likelihood idref="likelihood"/>
			</column>
			<column label="rootHeight" sf="6" width="12">
				<parameter idref="treeModel.rootHeight"/>
			</column>
			<column label="vc_rep1_new_CM.clock.rate" sf="6" width="12">
				<parameter idref="vc_rep1_new_CM.clock.rate"/>
			</column>
			<column label="Location.clock.rate" sf="6" width="12">
				<parameter idref="Location.clock.rate"/>
			</column>

			<!-- START Discrete Traits Model                                             -->

			<!-- END Discrete Traits Model                                               -->

		</log>

		<!-- write log to file                                                       -->
		<log id="fileLog" logEvery="5000" fileName="vc_rep1_new_CM_SC_BSR_phylo_simmNoBSSVS_50.log" overwrite="false">
			<posterior idref="posterior"/>
			<prior idref="prior"/>
			<likelihood idref="likelihood"/>
			<parameter idref="treeModel.rootHeight"/>
			<parameter idref="skyride.precision"/>
			<parameter idref="skyride.logPopSize"/>
			<parameter idref="skyride.groupSize"/>
			<parameter idref="kappa"/>
			<parameter idref="alpha"/>
			<parameter idref="vc_rep1_new_CM.clock.rate"/>
			<parameter idref="Location.clock.rate"/>
			<rateStatistic idref="vc_rep1_new_CM.meanRate"/>
			<rateStatistic idref="Location.meanRate"/>

			<!-- START Discrete Traits Model                                             -->
			<parameter idref="Location.rates"/>

			<!-- END Discrete Traits Model                                               -->

			<treeLikelihood idref="treeLikelihood"/>
			<strictClockBranchRates idref="vc_rep1_new_CM.branchRates"/>
			<strictClockBranchRates idref="Location.branchRates"/>

			<!-- START Discrete Traits Model                                             -->
			<ancestralTreeLikelihood idref="Location.treeLikelihood"/>

			<!-- END Discrete Traits Model                                               -->

			<gmrfSkyrideLikelihood idref="skyride"/>
		</log>

		<!-- START Discrete Traits Model                                             -->
		<log id="vc_rep1_new_CM_SC_BSR_phylo_simmNoBSSVS_50.LocationrateMatrixLog" logEvery="5000" fileName="vc_rep1_new_CM_SC_BSR_phylo_simmNoBSSVS_50.Location.rates.log">
			<parameter idref="Location.rates"/>
		</log>

		<!-- END Discrete Traits Model                                               -->


		<!-- write tree log to file                                                  -->
		<logTree id="treeFileLog" logEvery="5000" nexusFormat="true" fileName="vc_rep1_new_CM_SC_BSR_phylo_simmNoBSSVS_50.trees" sortTranslationTable="true">
			<treeModel idref="treeModel"/>
			<trait name="rate" tag="vc_rep1_new_CM.rate">
				<strictClockBranchRates idref="vc_rep1_new_CM.branchRates"/>
			</trait>
			<trait name="rate" tag="Location.rate">
				<strictClockBranchRates idref="Location.branchRates"/>
			</trait>
			<posterior idref="posterior"/>

			<!-- START Ancestral state reconstruction                                    -->
			<trait name="Location.states" tag="Location">
				<ancestralTreeLikelihood idref="Location.treeLikelihood"/>
			</trait>

			<!-- END Ancestral state reconstruction                                      -->

		</logTree>
	</mcmc>

	<!-- START Marginal Likelihood Estimator                                     -->

	<!-- Define marginal likelihood estimator (PS/SS) settings                   -->
	<marginalLikelihoodEstimator chainLength="1000000" pathSteps="100" pathScheme="betaquantile" alpha="0.3">
		<samplers>
			<mcmc idref="mcmc"/>
		</samplers>
		<pathLikelihood id="pathLikelihood">
			<source>
				<posterior idref="posterior"/>
			</source>
			<destination>
				<prior idref="prior"/>
			</destination>
		</pathLikelihood>
		<log id="MLELog" logEvery="1000" fileName="vc_rep1_new_CM_SC_BSR_phylo_simmNoBSSVS_50.mle.log">
			<pathLikelihood idref="pathLikelihood"/>
		</log>
	</marginalLikelihoodEstimator>

	<!-- Path sampling estimator from collected samples                          -->
	<pathSamplingAnalysis fileName="vc_rep1_new_CM_SC_BSR_phylo_simmNoBSSVS_50.mle.log">
		<likelihoodColumn name="pathLikelihood.delta"/>
		<thetaColumn name="pathLikelihood.theta"/>
	</pathSamplingAnalysis>

	<!-- Stepping-stone sampling estimator from collected samples                -->
	<steppingStoneSamplingAnalysis fileName="vc_rep1_new_CM_SC_BSR_phylo_simmNoBSSVS_50.mle.log">
		<likelihoodColumn name="pathLikelihood.delta"/>
		<thetaColumn name="pathLikelihood.theta"/>
	</steppingStoneSamplingAnalysis>

	<!-- END Marginal Likelihood Estimator                                       -->

	<report>
		<property name="timer">
			<mcmc idref="mcmc"/>
		</property>
	</report>
</beast>
